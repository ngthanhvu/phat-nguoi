      "backgroundColor": "#22c55e"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/logo_phat_nguoi.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_phat_nguoi.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ngthanhvu.tracuuphatnguoi"
    },
    "web": {
      "favicon": "./assets/logo_phat_nguoi.png"
    },
    "scheme": "datn",
    "extra": {
      "eas": {
        "projectId": "89be1196-52cb-4056-a7d3-26368a924d32"
      }
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
