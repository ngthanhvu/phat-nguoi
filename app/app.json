{
  "expo": {
    "name": "Tra cứu phạt nguội",
    "slug": "tra-cuu-phat-nguoi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_phat_nguoi.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
